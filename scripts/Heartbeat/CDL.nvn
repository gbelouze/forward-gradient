/// Convolutional dictionary learning

read "data/Heartbeat/X_train.csv" as X_train with
    SampleID: number
    TimeStamp: number
    X: number


table IDs[id] = by X_train.SampleID
expect table IDs small 205

table TimeStamp[ts] = by X_train.TimeStamp
expect table TimeStamp small 20k

table IDxTS = cross(IDs, TimeStamp)
IDxTS.X = sum(X_train.X)

autodiff IDs with
    params mean auto
    return sum((IDxTS.X - mean)^2)


show scalar "Mean" with
    mean
